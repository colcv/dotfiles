  "suggest.noselect": false,
  "suggest.enablePreselect": false,
  // "diagnostic.virtualText": true,

  "tree.openedIcon": "",
  "tree.closedIcon": "",
  "outline.detailAsDescription": false,
  "outline.showLineNumber": false,
  "outline.autoWidth": true,
  "outline.keepWindow": true,

  // "suggest.floatConfig": { "border": true },
  // "diagnostic.floatConfig": { "border": true },
  // "signature.floatConfig": { "border": true },
  // "hover.floatConfig": { "border": true },

  "list.statusLineSegments": [],
  "list.normalMappings": {
    "<C-v>": "action:vsplit",
    "<C-s>": "action:split",
    "<C-t>": "action:tabe",
    "<C-j>": "normal:j",
    "<C-k>": "normal:k"
  },
  "list.insertMappings": {
    "<C-v>": "action:vsplit",
    "<C-s>": "action:split",
    "<C-t>": "action:tabe"
  },

  "coc.preferences.formatOnSaveFiletypes": [
    "html",
    "css",
    "scss",
    "json",
    "jsonc",
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact",
    "lua"
  ],

  // lua
  "Lua.telemetry.enable": false,
  "Lua.hint.enable": false,
  "sumneko-lua.enableNvimLuaDev": true,
  "stylua.configPath": "/home/colcv/dotfiles/nvim/.config/nvim/stylua.toml",

  // coc-explorer
  "explorer.width": 40,
  "explorer.icon.expanded": "▸",
  "explorer.icon.collapsed": "▾",
  "explorer.icon.enableNerdfont": true,
  "explorer.icon.source": "nvim-web-devicons",
  "explorer.file.reveal.auto": true,
  "explorer.file.showHiddenFiles": true,
  "explorer.file.reveal.filter": {
    "literals": ["node_modules", ".git"]
  },
  "explorer.file.column.indent.indentLine": true,
  "explorer.filename.colored.enable": true,
  "explorer.buffer.showHiddenBuffers": true,
  "explorer.previewAction.onHover": false,
  "explorer.keyMappings.global": {
    "<C-v>": "open:vsplit",
    "<C-s>": "open:split",
    "<C-t>": "open:tab"
  },
  "explorer.focus": false,

  // emmet
  "emmet.priority": 99,

  // tsserver
  "javascript.suggest.completeFunctionCalls": false,
  "javascript.suggestionActions.enabled": false,
  "javascript.showUnused": false,
  // "javascript.inlayHints.variableTypes.enabled": true,
  // "javascript.inlayHints.parameterNames.enabled": "all",
  // "javascript.inlayHints.parameterTypes.enabled": true,
  // "javascript.inlayHints.enumMemberValues.enabled": true,
  // "javascript.inlayHints.functionLikeReturnTypes.enabled": true,
  // "javascript.inlayHints.propertyDeclarationTypes.enabled": true,
  // "javascript.inlayHints.parameterNames.suppressWhenArgumentMatchesName": true,

  // css
  "css.lint.unknownAtRules": "ignore",

  // prettier
  "prettier.singleQuote": true
}
